        xmlns:local="clr-namespace:Selection_car"
        mc:Ignorable="d"
        Title="To_NameId" Height="191.9" Width="289">
    <Grid Background="WhiteSmoke">
        <Grid.RowDefinitions>
            <RowDefinition Height="45*"/>
            <RowDefinition Height="101*"/>
        </Grid.RowDefinitions>
        <TextBlock HorizontalAlignment="Center" Height="32" Margin="33,10,32,0" TextWrapping="Wrap" Text="Введите ID или Имя пользователя" VerticalAlignment="Top" Width="137" />
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="72,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="130" Grid.Row="1"/>
        <Button x:Name="button" Content="Поиск" HorizontalAlignment="Left" Margin="72,55,0,0" VerticalAlignment="Top" Width="130" Click="button_Click" Background="Azure" Grid.Row="1"/>

    </Grid>
